{
  "manifest_version": 2,

  "name": "Wireless Audio Transfer",
  "icons": {
    "128": "icon.png"
  },
  "permissions": [
    "tabCapture",
    "*://159.203.114.155/*"
  ],
  "background": {
    "scripts": ["socket.io.js", "background.js"]
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }, 
  "description": "This extension allows you to play tab audio on another device.",
  "version": "1.0"
}
